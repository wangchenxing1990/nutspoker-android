<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
                android:layout_width="match_parent"
                android:paddingTop="10dp"
    android:layout_height="@dimen/quick_table_view_height_new"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@color/bg_normal">
    <LinearLayout
        android:id="@+id/game_code_container"
        android:layout_width="wrap_content"
        android:minWidth="154dp"
        android:layout_height="60dp"
        android:background="@mipmap/bg_invite_share_code"
        android:gravity="center_horizontal"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:orientation="vertical">
        <TextView
            android:id="@+id/tv_game_share_code"
            android:layout_width="wrap_content"
            android:layout_height="0dp"
            android:layout_weight="37"
            android:lines="1"
            android:paddingTop="15dp"
            android:paddingLeft="12dp"
            android:paddingRight="12dp"
            android:gravity="center"
            tools:text="111122"
            android:textColor="@color/black"
            android:alpha="0.32"
            android:textSize="19sp" />
        <TextView
            android:id="@+id/game_code_share"
            android:layout_width="wrap_content"
            android:layout_height="0dp"
            android:layout_weight="23"
            android:lines="1"
            android:gravity="center"
            android:includeFontPadding="false"
            android:text="@string/game_table_wait_clickshare"
            android:textColor="@color/black"
            android:alpha="0.32"
            android:textSize="11sp" />
    </LinearLayout>
    <LinearLayout
        android:id="@+id/ll_sng_rule"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_alignParentTop="true"
        android:gravity="center_horizontal"
        android:visibility="gone"
        tools:visibility="visible"
        android:orientation="vertical">
        <ImageView
            android:id="@+id/iv_game_sng"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@mipmap/icon_paiju_item_sng" />
        <ImageView
            android:id="@+id/iv_sng_rule"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:layout_marginTop="3dp"
            android:visibility="gone"
            tools:visibility="visible"
            android:src="@mipmap/icon_waring" />
    </LinearLayout>


    <!-- 普通局container -->
    <LinearLayout
        android:id="@+id/free_normal_game_container"
        android:layout_below="@id/game_code_container"
        android:visibility="gone"
        android:paddingTop="10dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:padding="5dp"
            android:orientation="horizontal">
            <FrameLayout
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical">
                <TextView
                    android:id="@+id/free_normal_blinds"
                    style="@style/paiju_list_item_small_text"
                    android:layout_gravity="center"
                    android:drawablePadding="3dp"
                    android:gravity="center"
                    android:drawableLeft="@mipmap/icon_club_paiju_chip"
                    tools:text=" 10000" />
            </FrameLayout>
            <FrameLayout
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical">
                <TextView
                    android:id="@+id/tv_discovery_match_duration"
                    style="@style/paiju_list_item_small_text"
                    android:layout_gravity="center"
                    android:drawablePadding="3dp"
                    android:gravity="center"
                    android:drawableLeft="@mipmap/icon_club_paiju_time"
                    tools:text=" 2小时" />
            </FrameLayout>
            <FrameLayout
                android:id="@+id/gamedesk_insurance_container"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical">
                <TextView
                    android:id="@+id/tv_gamedesk_insurance"
                    style="@style/paiju_list_item_small_text"
                    android:layout_gravity="center"
                    android:drawablePadding="3dp"
                    android:gravity="center"
                    android:drawableLeft="@mipmap/icon_chatroom_sliverdesk_insurance"
                    android:text="@string/game_create_config_insurance" />
            </FrameLayout>
            <FrameLayout
                android:id="@+id/gamedesk_ante_container"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical">
                <TextView
                    android:id="@+id/tv_gamedesk_ante"
                    style="@style/paiju_list_item_small_text"
                    android:layout_gravity="center"
                    android:drawablePadding="3dp"
                    android:gravity="center"
                    tools:text="Ante: 2" />
            </FrameLayout>
        </LinearLayout>
        <TextView
            android:id="@+id/tv_gamedesk_ip_and_gps"
            android:layout_width="match_parent"
            style="@style/paiju_list_item_small_text"
            android:visibility="gone"
            tools:visibility="visible"
            android:gravity="center"
            android:paddingTop="5dp"
            android:drawablePadding="3dp"
            android:text="IP限制    GPS限制" />
    </LinearLayout>






    <!-- sng局container -->
    <LinearLayout
        android:id="@+id/free_sng_game_container"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingTop="10dp"
        android:layout_below="@id/game_code_container"
        android:visibility="gone"
        tools:layout_below="@id/free_normal_game_container"
        android:orientation="vertical">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:padding="5dp"
            android:orientation="horizontal">
            <FrameLayout
                android:id="@+id/tv_discovery_member_container"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical">
                <TextView
                    android:id="@+id/tv_discovery_member"
                    style="@style/paiju_list_item_small_text"
                    android:layout_gravity="center"
                    android:drawablePadding="3dp"
                    android:drawableLeft="@mipmap/icon_club_paiju_member"
                    tools:text="9" />
            </FrameLayout>
            <FrameLayout
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical">
                <TextView
                    android:id="@+id/free_game_sng_chips"
                    style="@style/paiju_list_item_small_text"
                    android:layout_gravity="center"
                    android:drawablePadding="3dp"
                    android:gravity="center"
                    android:drawableLeft="@mipmap/icon_club_paiju_chip"
                    tools:text=" 10000" />
            </FrameLayout>
            <FrameLayout
                android:id="@+id/tv_discovery_match_checkin_fee_container"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical">
                <TextView
                    android:id="@+id/tv_discovery_match_checkin_fee"
                    style="@style/paiju_list_item_small_text"
                    android:layout_gravity="center"
                    android:drawablePadding="3dp"
                    android:lines="1"
                    android:ellipsize="end"
                    android:gravity="center"
                    android:drawableLeft="@mipmap/icon_club_paiju_checkin_fee"
                    tools:text="200+20" />
            </FrameLayout>
            <FrameLayout
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical">
                <TextView
                    android:id="@+id/free_sng_sng_blind_time"
                    style="@style/paiju_list_item_small_text"
                    android:layout_gravity="center"
                    android:drawablePadding="3dp"
                    android:gravity="center"
                    android:drawableLeft="@mipmap/icon_club_paiju_time_blind"
                    tools:text=" 2小时" />
            </FrameLayout>
        </LinearLayout>
        <TextView
            android:id="@+id/tv_gamedesk_ip_and_gps_sng"
            android:layout_width="match_parent"
            style="@style/paiju_list_item_small_text"
            android:visibility="gone"
            tools:visibility="visible"
            android:gravity="center"
            android:paddingTop="5dp"
            android:drawablePadding="3dp"
            android:text="IP限制    GPS限制" />
    </LinearLayout>

    <!--大菠萝-->
    <LinearLayout
        android:id="@+id/pineapple_container"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingTop="10dp"
        android:layout_below="@id/game_code_container"
        android:visibility="gone"
        tools:visibility="visible"
        tools:layout_below="@id/free_normal_game_container"
        android:orientation="vertical">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:padding="5dp"
            android:orientation="horizontal">
            <FrameLayout
                android:id="@+id/tv_pineapple_ante_container"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical">
                <TextView
                    android:id="@+id/tv_pineapple_ante_num"
                    style="@style/paiju_list_item_small_text"
                    android:layout_gravity="center"
                    android:drawablePadding="3dp"
                    android:drawableLeft="@mipmap/pineapple_ante"
                    tools:text="9" />
            </FrameLayout>
            <FrameLayout
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical">
                <TextView
                    android:id="@+id/pineapple_chips"
                    style="@style/paiju_list_item_small_text"
                    android:layout_gravity="center"
                    android:drawablePadding="3dp"
                    android:gravity="center"
                    android:drawableLeft="@mipmap/icon_club_paiju_chip"
                    tools:text=" 10000" />
            </FrameLayout>
            <FrameLayout
                android:id="@+id/pineapple_duration_container"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical">
                <TextView
                    android:id="@+id/tv_pineapple_duration"
                    style="@style/paiju_list_item_small_text"
                    android:layout_gravity="center"
                    android:drawablePadding="3dp"
                    android:lines="1"
                    android:ellipsize="end"
                    android:gravity="center"
                    android:drawableLeft="@mipmap/icon_club_paiju_time"
                    tools:text="20" />
            </FrameLayout>
        </LinearLayout>
        <TextView
            android:id="@+id/tv_pineapple_ip_and_gps_sng"
            android:layout_width="match_parent"
            style="@style/paiju_list_item_small_text"
            android:visibility="gone"
            tools:visibility="visible"
            android:gravity="center"
            android:paddingTop="5dp"
            android:drawablePadding="3dp"
            android:text="IP限制    GPS限制" />
    </LinearLayout>

    <ImageView
        android:id="@+id/iv_omaha_icon"
        android:layout_width="62dp"
        android:layout_height="22dp"
        android:visibility="gone"
        tools:visibility="visible"
        android:src="@mipmap/tag_omaha_2"/>
    <ImageView
        android:id="@+id/iv_pineapple_icon"
        android:layout_width="62dp"
        android:layout_height="20dp"
        android:visibility="gone"
        tools:visibility="visible"
        android:src="@mipmap/p_tags_ready_1"/>
</merge>