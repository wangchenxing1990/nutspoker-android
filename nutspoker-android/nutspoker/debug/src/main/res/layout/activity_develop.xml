<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <include layout="@layout/view_common_head" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:orientation="horizontal"
            android:paddingTop="5dp">

            <Button
                android:id="@+id/develop_btn_test"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginRight="10dp"
                android:layout_weight="1"
                android:background="@drawable/bg_login_btn"
                android:text="测试服" />

            <Button
                android:id="@+id/develop_btn_test2"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginRight="10dp"
                android:layout_weight="1"
                android:background="@drawable/bg_login_btn"
                android:enabled="false"
                android:text="测试服2" />

            <Button
                android:id="@+id/develop_btn_taiwan"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginRight="10dp"
                android:layout_weight="1"
                android:background="@drawable/bg_login_btn"
                android:enabled="false"
                android:text="台湾服" />

            <Button
                android:id="@+id/develop_btn_normal"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:background="@drawable/bg_login_btn"
                android:text="正式服" />
        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_marginTop="10dp"
            android:background="@color/login_solid_color" />

        <TextView
            android:id="@+id/develop_user_info_des"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:paddingTop="5dp"
            android:text="用户信息"
            android:textColor="@color/login_solid_color" />

        <TextView
            android:id="@+id/develop_uid"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:padding="2dp"
            android:text="uid: 10035" />

        <TextView
            android:id="@+id/develop_uuid"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:padding="2dp"
            android:text="uuid: 5653533" />

        <TextView
            android:id="@+id/develop_usernick"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:padding="2dp"
            android:text="昵称(nick): 暴走的苹果" />

        <TextView
            android:id="@+id/develop_phone"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:padding="2dp"
            android:text="phone: 13111111111" />

        <TextView
            android:id="@+id/develop_countryCode"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:padding="2dp"
            android:text="countryCode: 86" />

        <TextView
            android:id="@+id/develop_token"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:paddingBottom="2dp"
            android:paddingTop="2dp"
            android:text="token: 224324ddddddddddddddddddddddddddddddddddd"
            android:textSize="10dp" />

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_marginTop="5dp"
            android:background="@color/login_solid_color" />
        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_marginTop="5dp"
            android:background="@color/login_solid_color" />
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <EditText
                android:id="@+id/et_dev_web_url"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:hint="输入web地址"
                android:text="http://192.168.0.186/assets/nutspoker-other/banner/index.html"
                android:gravity="left"
                android:textColor="@color/text_select_color"
                android:textSize="10dp"
                android:layout_toLeftOf="@+id/btn_dev_web_url_go"
                android:textColorHint="@color/shop_text_no_select_color"
                android:layout_gravity="center_vertical|left"/>
            <Button
                android:id="@+id/btn_dev_web_url_go"
                android:layout_width="35dp"
                android:layout_height="35dp"
                android:onClick="testWebJump"
                android:layout_alignParentRight="true"
                android:background="@drawable/bg_login_btn"
                android:layout_gravity="right|center_vertical"
                android:text="跳转"/>
        </RelativeLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_marginTop="5dp"
            android:background="@color/login_solid_color" />
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingTop="2dp">
            <TextView
                android:text="user/amount"
                android:layout_width="wrap_content"
                android:layout_height="30dp"
                android:gravity="center_vertical"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:background="@drawable/bg_login_btn"
                android:id="@+id/do_http" />
            <TextView
                android:text="进入network_monitor"
                android:layout_width="wrap_content"
                android:layout_height="30dp"
                android:layout_marginTop="5dp"
                android:gravity="center_vertical"
                android:layout_below="@+id/do_http"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:background="@drawable/bg_login_btn"
                android:textSize="10dp"
                android:id="@+id/launch_chuck_directly" />
        </RelativeLayout>
        <Button
            android:id="@+id/btn_dev_test"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="测试按钮"/>
    </LinearLayout>
</ScrollView>